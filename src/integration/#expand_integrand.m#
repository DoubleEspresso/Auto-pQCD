#!/usr/local/bin/MathematicaScript -script
intReps = { ((J[1,2,0,0,0,1]->-0.00293872496827166616861770620516836061`5.,F[1,1,0,0,1,1]->0.02318103282864360237258694041884155013`5.,F[1,2,0,0,1,1]->0.00391828867903581349457775573341677955`5.,F[1,1,0,0,0,1]->0.02134760192176929360918791189760668203`5.,F[1,2,0,0,1,2]->0.00795025115388278517947195922488390352`5.,B[0,3,0,0,0,1]->-1/16/(Gamma[2])*Log[4*PI]*d^(-1)*PI^(-2)+1/16/(Gamma[2])*F[0]*d^(-1)*PI^(-2)+1/8/(Gamma[2])/(-4+d)*d^(-1)*PI^(-2)};
latticeIntegrand = 4*B[0,3,0,0,0,1]*I*OP+F[1,1,0,0,0,1]*I*OP+1/2*F[1,2,0,0,0,2]*I*DIM*OP+3*F[1,2,0,0,1,1]*I*OP+4*J[1,2,0,0,0,1]*I*OP-2*B[0,3,0,0,0,1]*I*DIM*OP-3/4*F[1,1,0,0,1,1]*I*OP-F[1,2,0,0,0,2]*I*OP-3/4*F[1,2,0,0,1,2]*I*OP-2*J[1,2,0,0,0,1]*I*DIM*OP;
(*===========================================================================================*)
ComputeNum[expr_]:=Module[{ans,rep},rep={Z0->0.154933390231060214084837208,Z1->0.107781313539874001343391550,F[0]->4.369225233874758};
			  ans=expr/.rep;
			  Return[ans];];


(*===========================================================================================*)
Expand\[Epsilon][expr_]:=Module[{ex1,ex2,ex3,ans,ans1,ans2,ans3,RepV,RepG,RepF},ex1=expr//.{DIM->4-2\[Epsilon],d->4-2 \[Epsilon],PI->\[Pi]}//Expand;
				ex2=Series[ex1,{\[Epsilon],0,1}]//Normal;
		  RepV={V[0]->1/(4 \[Pi]^2),V[1]->1/(8 \[Pi]^2),V[2]->3/(32 \[Pi]^2),V[3]->13/(128 \[Pi]^2),V[4]->77/(512 \[Pi]^2),V[5]->297/(1024 \[Pi]^2)};
RepG={G[0]->0,G[1]->-7/(256 \[Pi]^2),G[2]->-(11/(1536 \[Pi]^2)),G[3]->-(793/(589824 \[Pi]^2)),G[4]->-311/(1474560 \[Pi]^2)};
RepF={F[1]->F[0]-1/8 \[Pi]^2+35/12+\[Pi]^2/2 Z1,F[2]->F[0]-31/144 \[Pi]^2+1349/216+1/9 \[Pi]^2 Z0+31/36 \[Pi]^2 Z1,F[3]->F[0]-523/1872 \[Pi]^2+24257/2808+25/117 \[Pi]^2 Z0+523/468 \[Pi]^2 Z1,F[4]->F[0]-7145/22176 \[Pi]^2+294919/33264+401/1386 \[Pi]^2 Z0+7145/5544 \[Pi]^2 Z1};
ans1=ex2/.RepV;
ans2=ans1/.RepG;
ans3=ans2/.RepF;
Return[Expand[ComputeNum[ans3]]] ;
];


(*===========================================================================================*)
ans1=latticeIntegrand//.intReps;
ans2=16 \[Pi]^2*Expand\[Epsilon][ans1]//.{1/\[Epsilon]->1/EP,1/EPS->1/EP,EPS->0,\[Epsilon]->0,delta->0};
ans3=Expand[ans2]//.{0.*I->0,0.->0};

				  Print[ToString[(1/(16\[\Pi]^2))Expand[ans3],FormatType->InputForm,CharacterEncoding->"ASCII"]];

Exit[];


