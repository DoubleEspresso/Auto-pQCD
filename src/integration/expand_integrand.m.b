RepG = {
  G[0] -> 0,
  G[1] -> -7/(256 \[Pi]^2),
  G[2] -> -(11/(1536 \[Pi]^2)),
     G[3] -> -(793/(589824 \[Pi]^2)),
     G[4] -> -311/(1474560 \[Pi]^2)};
   
RepF = {
  F[1] -> F[0] - 1/8 \[Pi]^2 + 35/12 + \[Pi]^2/2 Z1, 
  F[2] -> F[0] - 31/144 \[Pi]^2 + 1349/216 + 1/9 \[Pi]^2 Z0 + 31/36 \[Pi]^2 Z1, 
  F[3] -> F[0] - 523/1872 \[Pi]^2 + 24257/2808 + 25/117 \[Pi]^2 Z0 + 523/468 \[Pi]^2 Z1,
  F[4] -> F[0] - 7145/22176 \[Pi]^2 + 294919/33264 + 401/1386 \[Pi]^2 Z0 + 7145/5544 \[Pi]^2 Z1};

