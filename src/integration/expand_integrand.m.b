Expand\[Epsilon][expr_] := Module[{ex1, ex2, ex3, ans, ans1, ans2, ans3, RepV, RepG, RepF},
	ex1 = expr //. {DIM->4-2\[Epsilon], d -> 4 - 2 \[Epsilon]} // Expand;
	ex2 = Series[ex1, {\[Epsilon], 0, 1}] // Normal;
   
  RepV = {V[0] -> 1/(4 \[Pi]^2),
